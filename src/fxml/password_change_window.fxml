<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Password_Change_Controller">
   <children>
      <Label fx:id="label_title" alignment="CENTER" layoutX="199.0" layoutY="41.0" prefHeight="55.0" prefWidth="202.0" text="Zmiana Hasła">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <PasswordField fx:id="current_password" layoutX="95.0" layoutY="119.0" prefHeight="39.0" prefWidth="439.0" promptText="Obecne hasło">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="new_password" layoutX="95.0" layoutY="188.0" prefHeight="39.0" prefWidth="439.0" promptText="Nowe hasło">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="confirm_new_password" layoutX="95.0" layoutY="250.0" prefHeight="39.0" prefWidth="439.0" promptText="Potwierdź nowe hasło">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </PasswordField>
      <Button layoutX="125.0" layoutY="311.0" mnemonicParsing="false" onAction="#button_confirm" prefHeight="68.0" prefWidth="175.0" text="Potwierdź" />
      <Button layoutX="314.0" layoutY="311.0" mnemonicParsing="false" onAction="#button_cancel" prefHeight="68.0" prefWidth="175.0" text="Anuluj" />
   </children>
</AnchorPane>
